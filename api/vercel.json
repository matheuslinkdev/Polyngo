{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/idiomas", "dest": "index.js" },
    { "src": "/formacoes", "dest": "index.js" },
    { "src": "/idiomas/(.*)", "dest": "index.js" },
    { "src": "/login", "dest": "index.js" },
    { "src": "/signup", "dest": "index.js" },
    { "src": "/users/(.*)", "dest": "index.js" }
  ]
}
